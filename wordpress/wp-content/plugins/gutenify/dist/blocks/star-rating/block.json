{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "category": "gutenify",
  "name": "gutenify/star-rating",
  "title": "Star rating",
  "version": "2",
  "attributes": {
    "blockClientId": {
      "type": "string",
      "default": ""
    },
    "blockAdvanceOptions": {
      "type": "object",
      "default": {
        "textColor": "#ff9800"
      }
    },
    "rating": {
      "type": "number",
      "default": 5
    },
    "textAlignment": {
      "type": "string",
      "default": "center"
    }
  },
  "editorScript": "gutenify-star-rating-editor",
  "editorStyle": "gutenify-star-rating-editor",
  "style": "gutenify-star-rating-frontend"
}